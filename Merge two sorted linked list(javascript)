function mergeLists(head1, head2) {
    var merged = new SinglyLinkedListNode();
    
    let curr1 = head1, prev1
    let curr2 = head2, prev2
    
    if(head1 == null){
        return head2
    }else if(head2 == null){
        return head1
    }
    
    
if (head1.data <= head2.data) {
    merged = head1;
    head1 = head1.next;
  } else {
    merged = head2;
    head2 = head2.next;
  }
  
var mergedTail = new SinglyLinkedListNode();
var temp = new SinglyLinkedListNode();
mergedTail = merged
  
  while (head1 != null && head2 != null) {

    if (head1.data <= head2.data) {
      temp = head1;
      head1 = head1.next;
    } else {
      temp = head2;
      head2 = head2.next;
    }
   
    mergedTail.next = temp;
    mergedTail = temp;
  }

  if (head1 != null) {
    mergedTail.next = head1;
  } else if (head2 != null) {
    mergedTail.next = head2;
  }
  return merged;

}
